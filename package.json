{
  "name": "grok-style-chat-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "node server/index.js",
    "dev:full": "concurrently -n \"server,web\" -c \"cyan,magenta\" \"npm:dev:server\" \"npm:dev\"",
    "build": "tsc --noEmit && vite build",
