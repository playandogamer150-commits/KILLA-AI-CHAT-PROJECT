    "secrets:scan:staged": "node scripts/precommit-secret-scan.mjs"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.60.1",
    "@clerk/express": "^1.7.70",
    "@llamaindex/ollama": "^0.1.23",
    "@llamaindex/openai": "^0.4.22",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "llamaindex": "^0.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.1.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
